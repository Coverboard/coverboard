<!doctype html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title><%= @project_name %></title>

  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body class="page">
  <header class="page__header">
    <%= @project_name %>
  </header>
  <section class="graphs">
    <div class="time-graph">
    </div>
    <div class="state-graphs">
    </div>
  </section>

  <script type="text/template" id="bar-graph-template">
    <%= render 'bar_graph' %>
  </script>
  <script type="text/template" id="state-graph-template">
    <%= render 'state_graph' %>
  </script>
  <script type="text/template" id="not-found-template">
    <%= render 'not_found' %>
  </script>
  <%= render partial: 'shared/ga' %>
</body>
</html>
